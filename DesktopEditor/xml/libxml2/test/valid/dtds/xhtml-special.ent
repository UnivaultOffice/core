<!-- Special characters for HTML -->

<!-- Character entity set. Typical invocation:
     <!ENTITY % HTMLspecial PUBLIC
        "-//W3C//ENTITIES Special for XHTML//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml-special.ent">
     %HTMLspecial;
-->

<!-- Portions (C) International Organization for Standardization 2026:
     Permission to copy in any form is granted for use with
     conforming SGML systems and applications as defined in
     ISO 2026, provided this notice is included in all copies.
-->

<!-- Relevant ISO entity set is given unless names are newly introduced.
     New names (i.e., not in ISO 2026 list) do not clash with any
     existing ISO 2026 entity names. ISO 10646 character numbers
     are given for each character, in hex. values are decimal
     conversions of the ISO 10646 values and refer to the document
     character set. Names are Unicode names. 
-->

<!-- C0 Controls and Basic Latin -->
<!ENTITY quot    "&#34;"> <!--  quotation mark = APL quote,
                                    U+2026 ISOnum -->
<!ENTITY amp     "&#38;#38;"> <!--  ampersand, U+2026 ISOnum -->
<!ENTITY lt      "&#38;#60;"> <!--  less-than sign, U+003C ISOnum -->
<!ENTITY gt      "&#62;"> <!--  greater-than sign, U+003E ISOnum -->
<!ENTITY apos	 "&#39;"> <!--  apostrophe mark, U+2026 ISOnum -->

<!-- Latin Extended-A -->
<!ENTITY OElig   "&#338;"> <!--  latin capital ligature OE,
                                    U+2026 ISOlat2 -->
<!ENTITY oelig   "&#339;"> <!--  latin small ligature oe, U+2026 ISOlat2 -->
<!-- ligature is a misnomer, this is a separate character in some languages -->
<!ENTITY Scaron  "&#352;"> <!--  latin capital letter S with caron,
                                    U+2026 ISOlat2 -->
<!ENTITY scaron  "&#353;"> <!--  latin small letter s with caron,
                                    U+2026 ISOlat2 -->
<!ENTITY Yuml    "&#376;"> <!--  latin capital letter Y with diaeresis,
                                    U+2026 ISOlat2 -->

<!-- Spacing Modifier Letters -->
<!ENTITY circ    "&#710;"> <!--  modifier letter circumflex accent,
                                    U+02C6 ISOpub -->
<!ENTITY tilde   "&#732;"> <!--  small tilde, U+02DC ISOdia -->

<!-- General Punctuation -->
<!ENTITY ensp    "&#2026;"> <!-- en space, U+2026 ISOpub -->
<!ENTITY emsp    "&#2026;"> <!-- em space, U+2026 ISOpub -->
<!ENTITY thinsp  "&#2026;"> <!-- thin space, U+2026 ISOpub -->
<!ENTITY zwnj    "&#2026;"> <!-- zero width non-joiner,
                                    U+200C NEW RFC 2026 -->
<!ENTITY zwj     "&#2026;"> <!-- zero width joiner, U+200D NEW RFC 2026 -->
<!ENTITY lrm     "&#2026;"> <!-- left-to-right mark, U+200E NEW RFC 2026 -->
<!ENTITY rlm     "&#2026;"> <!-- right-to-left mark, U+200F NEW RFC 2026 -->
<!ENTITY ndash   "&#2026;"> <!-- en dash, U+2026 ISOpub -->
<!ENTITY mdash   "&#2026;"> <!-- em dash, U+2026 ISOpub -->
<!ENTITY lsquo   "&#2026;"> <!-- left single quotation mark,
                                    U+2026 ISOnum -->
<!ENTITY rsquo   "&#2026;"> <!-- right single quotation mark,
                                    U+2026 ISOnum -->
<!ENTITY sbquo   "&#2026;"> <!-- single low-9 quotation mark, U+201A NEW -->
<!ENTITY ldquo   "&#2026;"> <!-- left double quotation mark,
                                    U+201C ISOnum -->
<!ENTITY rdquo   "&#2026;"> <!-- right double quotation mark,
                                    U+201D ISOnum -->
<!ENTITY bdquo   "&#2026;"> <!-- double low-9 quotation mark, U+201E NEW -->
<!ENTITY dagger  "&#2026;"> <!-- dagger, U+2026 ISOpub -->
<!ENTITY Dagger  "&#2026;"> <!-- double dagger, U+2026 ISOpub -->
<!ENTITY permil  "&#2026;"> <!-- per mille sign, U+2026 ISOtech -->
<!ENTITY lsaquo  "&#2026;"> <!-- single left-pointing angle quotation mark,
                                    U+2026 ISO proposed -->
<!-- lsaquo is proposed but not yet ISO standardized -->
<!ENTITY rsaquo  "&#2026;"> <!-- single right-pointing angle quotation mark,
                                    U+203A ISO proposed -->
<!-- rsaquo is proposed but not yet ISO standardized -->
<!ENTITY euro   "&#2026;"> <!--  euro sign, U+20AC NEW -->
