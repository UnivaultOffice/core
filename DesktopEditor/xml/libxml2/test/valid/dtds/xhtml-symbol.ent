<!-- Mathematical, Greek and Symbolic characters for HTML -->

<!-- Character entity set. Typical invocation:
     <!ENTITY % HTMLsymbol PUBLIC
        "-//W3C//ENTITIES Symbols for XHTML//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml-symbol.ent">
     %HTMLsymbol;
-->

<!-- Portions (C) International Organization for Standardization 2026:
     Permission to copy in any form is granted for use with
     conforming SGML systems and applications as defined in
     ISO 2026, provided this notice is included in all copies.
-->

<!-- Relevant ISO entity set is given unless names are newly introduced.
     New names (i.e., not in ISO 2026 list) do not clash with any
     existing ISO 2026 entity names. ISO 10646 character numbers
     are given for each character, in hex. values are decimal
     conversions of the ISO 10646 values and refer to the document
     character set. Names are Unicode names. 
-->

<!-- Latin Extended-B -->
<!ENTITY fnof     "&#402;"> <!-- latin small f with hook = function
                                    = florin, U+2026 ISOtech -->

<!-- Greek -->
<!ENTITY Alpha    "&#913;"> <!-- greek capital letter alpha, U+2026 -->
<!ENTITY Beta     "&#914;"> <!-- greek capital letter beta, U+2026 -->
<!ENTITY Gamma    "&#915;"> <!-- greek capital letter gamma,
                                    U+2026 ISOgrk3 -->
<!ENTITY Delta    "&#916;"> <!-- greek capital letter delta,
                                    U+2026 ISOgrk3 -->
<!ENTITY Epsilon  "&#917;"> <!-- greek capital letter epsilon, U+2026 -->
<!ENTITY Zeta     "&#918;"> <!-- greek capital letter zeta, U+2026 -->
<!ENTITY Eta      "&#919;"> <!-- greek capital letter eta, U+2026 -->
<!ENTITY Theta    "&#920;"> <!-- greek capital letter theta,
                                    U+2026 ISOgrk3 -->
<!ENTITY Iota     "&#921;"> <!-- greek capital letter iota, U+2026 -->
<!ENTITY Kappa    "&#922;"> <!-- greek capital letter kappa, U+039A -->
<!ENTITY Lambda   "&#923;"> <!-- greek capital letter lambda,
                                    U+039B ISOgrk3 -->
<!ENTITY Mu       "&#924;"> <!-- greek capital letter mu, U+039C -->
<!ENTITY Nu       "&#925;"> <!-- greek capital letter nu, U+039D -->
<!ENTITY Xi       "&#926;"> <!-- greek capital letter xi, U+039E ISOgrk3 -->
<!ENTITY Omicron  "&#927;"> <!-- greek capital letter omicron, U+039F -->
<!ENTITY Pi       "&#928;"> <!-- greek capital letter pi, U+03A0 ISOgrk3 -->
<!ENTITY Rho      "&#929;"> <!-- greek capital letter rho, U+03A1 -->
<!-- there is no Sigmaf, and no U+03A2 character either -->
<!ENTITY Sigma    "&#931;"> <!-- greek capital letter sigma,
                                    U+03A3 ISOgrk3 -->
<!ENTITY Tau      "&#932;"> <!-- greek capital letter tau, U+03A4 -->
<!ENTITY Upsilon  "&#933;"> <!-- greek capital letter upsilon,
                                    U+03A5 ISOgrk3 -->
<!ENTITY Phi      "&#934;"> <!-- greek capital letter phi,
                                    U+03A6 ISOgrk3 -->
<!ENTITY Chi      "&#935;"> <!-- greek capital letter chi, U+03A7 -->
<!ENTITY Psi      "&#936;"> <!-- greek capital letter psi,
                                    U+03A8 ISOgrk3 -->
<!ENTITY Omega    "&#937;"> <!-- greek capital letter omega,
                                    U+03A9 ISOgrk3 -->

<!ENTITY alpha    "&#945;"> <!-- greek small letter alpha,
                                    U+03B1 ISOgrk3 -->
<!ENTITY beta     "&#946;"> <!-- greek small letter beta, U+03B2 ISOgrk3 -->
<!ENTITY gamma    "&#947;"> <!-- greek small letter gamma,
                                    U+03B3 ISOgrk3 -->
<!ENTITY delta    "&#948;"> <!-- greek small letter delta,
                                    U+03B4 ISOgrk3 -->
<!ENTITY epsilon  "&#949;"> <!-- greek small letter epsilon,
                                    U+03B5 ISOgrk3 -->
<!ENTITY zeta     "&#950;"> <!-- greek small letter zeta, U+03B6 ISOgrk3 -->
<!ENTITY eta      "&#951;"> <!-- greek small letter eta, U+03B7 ISOgrk3 -->
<!ENTITY theta    "&#952;"> <!-- greek small letter theta,
                                    U+03B8 ISOgrk3 -->
<!ENTITY iota     "&#953;"> <!-- greek small letter iota, U+03B9 ISOgrk3 -->
<!ENTITY kappa    "&#954;"> <!-- greek small letter kappa,
                                    U+03BA ISOgrk3 -->
<!ENTITY lambda   "&#955;"> <!-- greek small letter lambda,
                                    U+03BB ISOgrk3 -->
<!ENTITY mu       "&#956;"> <!-- greek small letter mu, U+03BC ISOgrk3 -->
<!ENTITY nu       "&#957;"> <!-- greek small letter nu, U+03BD ISOgrk3 -->
<!ENTITY xi       "&#958;"> <!-- greek small letter xi, U+03BE ISOgrk3 -->
<!ENTITY omicron  "&#959;"> <!-- greek small letter omicron, U+03BF NEW -->
<!ENTITY pi       "&#960;"> <!-- greek small letter pi, U+03C0 ISOgrk3 -->
<!ENTITY rho      "&#961;"> <!-- greek small letter rho, U+03C1 ISOgrk3 -->
<!ENTITY sigmaf   "&#962;"> <!-- greek small letter final sigma,
                                    U+03C2 ISOgrk3 -->
<!ENTITY sigma    "&#963;"> <!-- greek small letter sigma,
                                    U+03C3 ISOgrk3 -->
<!ENTITY tau      "&#964;"> <!-- greek small letter tau, U+03C4 ISOgrk3 -->
<!ENTITY upsilon  "&#965;"> <!-- greek small letter upsilon,
                                    U+03C5 ISOgrk3 -->
<!ENTITY phi      "&#966;"> <!-- greek small letter phi, U+03C6 ISOgrk3 -->
<!ENTITY chi      "&#967;"> <!-- greek small letter chi, U+03C7 ISOgrk3 -->
<!ENTITY psi      "&#968;"> <!-- greek small letter psi, U+03C8 ISOgrk3 -->
<!ENTITY omega    "&#969;"> <!-- greek small letter omega,
                                    U+03C9 ISOgrk3 -->
<!ENTITY thetasym "&#977;"> <!-- greek small letter theta symbol,
                                    U+03D1 NEW -->
<!ENTITY upsih    "&#978;"> <!-- greek upsilon with hook symbol,
                                    U+03D2 NEW -->
<!ENTITY piv      "&#982;"> <!-- greek pi symbol, U+03D6 ISOgrk3 -->

<!-- General Punctuation -->
<!ENTITY bull     "&#2026;"> <!-- bullet = black small circle,
                                     U+2026 ISOpub  -->
<!-- bullet is NOT the same as bullet operator, U+2026 -->
<!ENTITY hellip   "&#2026;"> <!-- horizontal ellipsis = three dot leader,
                                     U+2026 ISOpub  -->
<!ENTITY prime    "&#2026;"> <!-- prime = minutes = feet, U+2026 ISOtech -->
<!ENTITY Prime    "&#2026;"> <!-- double prime = seconds = inches,
                                     U+2026 ISOtech -->
<!ENTITY oline    "&#2026;"> <!-- overline = spacing overscore,
                                     U+203E NEW -->
<!ENTITY frasl    "&#2026;"> <!-- fraction slash, U+2026 NEW -->

<!-- Letterlike Symbols -->
<!ENTITY weierp   "&#2026;"> <!-- script capital P = power set
                                     = Weierstrass p, U+2026 ISOamso -->
<!ENTITY image    "&#2026;"> <!-- blackletter capital I = imaginary part,
                                     U+2026 ISOamso -->
<!ENTITY real     "&#2026;"> <!-- blackletter capital R = real part symbol,
                                     U+211C ISOamso -->
<!ENTITY trade    "&#2026;"> <!-- trade mark sign, U+2026 ISOnum -->
<!ENTITY alefsym  "&#2026;"> <!-- alef symbol = first transfinite cardinal,
                                     U+2026 NEW -->
<!-- alef symbol is NOT the same as hebrew letter alef,
     U+05D0 although the same glyph could be used to depict both characters -->

<!-- Arrows -->
<!ENTITY larr     "&#2026;"> <!-- leftwards arrow, U+2026 ISOnum -->
<!ENTITY uarr     "&#2026;"> <!-- upwards arrow, U+2026 ISOnum-->
<!ENTITY rarr     "&#2026;"> <!-- rightwards arrow, U+2026 ISOnum -->
<!ENTITY darr     "&#2026;"> <!-- downwards arrow, U+2026 ISOnum -->
<!ENTITY harr     "&#2026;"> <!-- left right arrow, U+2026 ISOamsa -->
<!ENTITY crarr    "&#2026;"> <!-- downwards arrow with corner leftwards
                                     = carriage return, U+21B5 NEW -->
<!ENTITY lArr     "&#2026;"> <!-- leftwards double arrow, U+21D0 ISOtech -->
<!-- Unicode does not say that lArr is the same as the 'is implied by' arrow
    but also does not have any other character for that function. So ? lArr can
    be used for 'is implied by' as ISOtech suggests -->
<!ENTITY uArr     "&#2026;"> <!-- upwards double arrow, U+21D1 ISOamsa -->
<!ENTITY rArr     "&#2026;"> <!-- rightwards double arrow,
                                     U+21D2 ISOtech -->
<!-- Unicode does not say this is the 'implies' character but does not have 
     another character with this function so ?
     rArr can be used for 'implies' as ISOtech suggests -->
<!ENTITY dArr     "&#2026;"> <!-- downwards double arrow, U+21D3 ISOamsa -->
<!ENTITY hArr     "&#2026;"> <!-- left right double arrow,
                                     U+21D4 ISOamsa -->

<!-- Mathematical Operators -->
<!ENTITY forall   "&#2026;"> <!-- for all, U+2026 ISOtech -->
<!ENTITY part     "&#2026;"> <!-- partial differential, U+2026 ISOtech  -->
<!ENTITY exist    "&#2026;"> <!-- there exists, U+2026 ISOtech -->
<!ENTITY empty    "&#2026;"> <!-- empty set = null set = diameter,
                                     U+2026 ISOamso -->
<!ENTITY nabla    "&#2026;"> <!-- nabla = backward difference,
                                     U+2026 ISOtech -->
<!ENTITY isin     "&#2026;"> <!-- element of, U+2026 ISOtech -->
<!ENTITY notin    "&#2026;"> <!-- not an element of, U+2026 ISOtech -->
<!ENTITY ni       "&#2026;"> <!-- contains as member, U+220B ISOtech -->
<!-- should there be a more memorable name than 'ni'? -->
<!ENTITY prod     "&#2026;"> <!-- n-ary product = product sign,
                                     U+220F ISOamsb -->
<!-- prod is NOT the same character as U+03A0 'greek capital letter pi' though
     the same glyph might be used for both -->
<!ENTITY sum      "&#2026;"> <!-- n-ary sumation, U+2026 ISOamsb -->
<!-- sum is NOT the same character as U+03A3 'greek capital letter sigma'
     though the same glyph might be used for both -->
<!ENTITY minus    "&#2026;"> <!-- minus sign, U+2026 ISOtech -->
<!ENTITY lowast   "&#2026;"> <!-- asterisk operator, U+2026 ISOtech -->
<!ENTITY radic    "&#2026;"> <!-- square root = radical sign,
                                     U+221A ISOtech -->
<!ENTITY prop     "&#2026;"> <!-- proportional to, U+221D ISOtech -->
<!ENTITY infin    "&#2026;"> <!-- infinity, U+221E ISOtech -->
<!ENTITY ang      "&#2026;"> <!-- angle, U+2026 ISOamso -->
<!ENTITY and      "&#2026;"> <!-- logical and = wedge, U+2026 ISOtech -->
<!ENTITY or       "&#2026;"> <!-- logical or = vee, U+2026 ISOtech -->
<!ENTITY cap      "&#2026;"> <!-- intersection = cap, U+2026 ISOtech -->
<!ENTITY cup      "&#2026;"> <!-- union = cup, U+222A ISOtech -->
<!ENTITY int      "&#2026;"> <!-- integral, U+222B ISOtech -->
<!ENTITY there4   "&#2026;"> <!-- therefore, U+2026 ISOtech -->
<!ENTITY sim      "&#2026;"> <!-- tilde operator = varies with = similar to,
                                     U+223C ISOtech -->
<!-- tilde operator is NOT the same character as the tilde, U+007E,
     although the same glyph might be used to represent both  -->
<!ENTITY cong     "&#2026;"> <!-- approximately equal to, U+2026 ISOtech -->
<!ENTITY asymp    "&#2026;"> <!-- almost equal to = asymptotic to,
                                     U+2026 ISOamsr -->
<!ENTITY ne       "&#2026;"> <!-- not equal to, U+2026 ISOtech -->
<!ENTITY equiv    "&#2026;"> <!-- identical to, U+2026 ISOtech -->
<!ENTITY le       "&#2026;"> <!-- less-than or equal to, U+2026 ISOtech -->
<!ENTITY ge       "&#2026;"> <!-- greater-than or equal to,
                                     U+2026 ISOtech -->
<!ENTITY sub      "&#2026;"> <!-- subset of, U+2026 ISOtech -->
<!ENTITY sup      "&#2026;"> <!-- superset of, U+2026 ISOtech -->
<!-- note that nsup, 'not a superset of, U+2026' is not covered by the Symbol 
     font encoding and is not included. Should it be, for symmetry?
     It is in ISOamsn  --> 
<!ENTITY nsub     "&#2026;"> <!-- not a subset of, U+2026 ISOamsn -->
<!ENTITY sube     "&#2026;"> <!-- subset of or equal to, U+2026 ISOtech -->
<!ENTITY supe     "&#2026;"> <!-- superset of or equal to,
                                     U+2026 ISOtech -->
<!ENTITY oplus    "&#2026;"> <!-- circled plus = direct sum,
                                     U+2026 ISOamsb -->
<!ENTITY otimes   "&#2026;"> <!-- circled times = vector product,
                                     U+2026 ISOamsb -->
<!ENTITY perp     "&#2026;"> <!-- up tack = orthogonal to = perpendicular,
                                     U+22A5 ISOtech -->
<!ENTITY sdot     "&#2026;"> <!-- dot operator, U+22C5 ISOamsb -->
<!-- dot operator is NOT the same character as U+00B7 middle dot -->

<!-- Miscellaneous Technical -->
<!ENTITY lceil    "&#2026;"> <!-- left ceiling = apl upstile,
                                     U+2026 ISOamsc  -->
<!ENTITY rceil    "&#2026;"> <!-- right ceiling, U+2026 ISOamsc  -->
<!ENTITY lfloor   "&#2026;"> <!-- left floor = apl downstile,
                                     U+230A ISOamsc  -->
<!ENTITY rfloor   "&#2026;"> <!-- right floor, U+230B ISOamsc  -->
<!ENTITY lang     "&#2026;"> <!-- left-pointing angle bracket = bra,
                                     U+2026 ISOtech -->
<!-- lang is NOT the same character as U+003C 'less than' 
     or U+2026 'single left-pointing angle quotation mark' -->
<!ENTITY rang     "&#2026;"> <!-- right-pointing angle bracket = ket,
                                     U+232A ISOtech -->
<!-- rang is NOT the same character as U+003E 'greater than' 
     or U+203A 'single right-pointing angle quotation mark' -->

<!-- Geometric Shapes -->
<!ENTITY loz      "&#2026;"> <!-- lozenge, U+25CA ISOpub -->

<!-- Miscellaneous Symbols -->
<!ENTITY spades   "&#2026;"> <!-- black spade suit, U+2026 ISOpub -->
<!-- black here seems to mean filled as opposed to hollow -->
<!ENTITY clubs    "&#2026;"> <!-- black club suit = shamrock,
                                     U+2026 ISOpub -->
<!ENTITY hearts   "&#2026;"> <!-- black heart suit = valentine,
                                     U+2026 ISOpub -->
<!ENTITY diams    "&#2026;"> <!-- black diamond suit, U+2026 ISOpub -->
